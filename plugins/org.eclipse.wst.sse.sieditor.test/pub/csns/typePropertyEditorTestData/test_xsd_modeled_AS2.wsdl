		<jaxws:bindings>
			<jaxws:class name="AS2SEI" />
		</jaxws:bindings>
		<operation name="op2">
			<input message="tns:op2" />
			<output message="tns:op2Response" />
		</operation>
	</portType>
	<binding name="AS2Binding" type="tns:AS2">
