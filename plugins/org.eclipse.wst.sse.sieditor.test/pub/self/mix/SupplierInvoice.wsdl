								<xsd:element name="StreetName" minOccurs="0">
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:maxLength value="60" />
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
								<xsd:element name="StreetPrefixName" minOccurs="0"
									maxOccurs="unbounded" />
								<xsd:element name="StreetSuffixName" minOccurs="0"
									maxOccurs="unbounded" />
