									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="Address" type="WebAddress" />
											<xsd:element name="AddressDefaultIndicator" type="xsd:boolean" />
											<xsd:element name="AddressDescription" minOccurs="0"
												maxOccurs="unbounded" type="Description" />
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
